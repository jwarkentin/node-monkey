{
  "name": "node-monkey",
  "version": "1.1.4",
  "description": "A tool for inspecting, debugging and commanding Node applications through a web browser or SSH interface into your app",
  "keywords": [
    "inspect",
    "debug",
    "debugging",
    "console",
    "log",
    "profile",
    "profiler"
  ],
  "main": "./dist/server.js",
  "scripts": {
    "build": "webpack",
    "build:dev": "webpack --watch",
    "start": "babel-node --es-module-specifier-resolution=node src/server/index.js",
    "lint": "eslint src/**/*.js",
    "lint:fix": "npm run lint -- --fix"
  },
  "author": "Justin Warkentin <justin.warkentin@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/jwarkentin/node-monkey",
  "bugs": "https://github.com/jwarkentin/node-monkey/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/jwarkentin/node-monkey.git"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.2",
    "keypair": "^1.0.4",
    "lodash": "^4.17.21",
    "minimist": "^1.2.5",
    "node-pty": "^0.10.1",
    "scrypt-kdf": "^2.0.1",
    "socket.io": "^4.4.1",
    "source-map-support": "^0.5.21",
    "ssh2": "^1.7.0",
    "terminal-kit": "^2.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/node": "^7.16.8",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.17.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-source-map-support": "^2.1.1",
    "bunyan": "^1.8.15",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "html-webpack-plugin": "^5.5.0",
    "ignore-loader": "^0.1.2",
    "prettier": "^2.5.1",
    "terser-webpack-plugin": "^5.3.1",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0"
  }
}
